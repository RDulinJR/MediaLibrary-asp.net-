<configuration>

    <connectionStrings>
        <add name="MediaLibraryConnectionString1" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\MediaLibrary.mdf;Integrated Security=True"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.web>
      <pages theme="BlueGreen" styleSheetTheme="BlueGreen"></pages>
      <compilation debug="true" targetFramework="4.5.1" />
      <httpRuntime targetFramework="4.5.1" />
      <authentication mode="Forms">
        <forms name="libraryApp"
               loginUrl="~/Html/Login.aspx"
               defaultUrl="~/Html/Default.aspx">
          
        </forms>
      </authentication>
    </system.web>

</configuration>
